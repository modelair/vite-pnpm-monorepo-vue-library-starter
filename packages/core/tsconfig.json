{
    "extends": "../../tsconfig.base.json",
  "compilerOptions": {

    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": [
    ],
    "useDefineForClassFields": false,
    "paths": {
      "@r41/core": [
        "./src"
      ],

    }
  "include": [
    "src/*",
    "src/**/*",
    "src/**/**/*",
    "types.d.ts",
    "global.d.ts",
    "env.d.ts",
    "vite.d.ts",
    "vitest/globals"
  ],
  "exclude": [
    "node_modules",
  ]
}
