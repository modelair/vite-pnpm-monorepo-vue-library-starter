    //    "src/**/*.d.ts",
    //    "src/**/*.tsx",
    //    "src/**/*.vue",
    //    "src/index.ts",
    //    "types.d.ts"
    //  ],
  },
  "include": [
    //    "src/renderComponent/list-checkbox.vue"
    "src/*",
